{
  "name": "Add JXL support",
  "description": "Converts JXL images so you can view them in the browser.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
       "16": "logo/16x16.png",
	   "19": "logo/19x19.png",
	   "38": "logo/38x38.png",
       "48": "logo/48x48.png",
      "128": "logo/128x128.png"
  },
  "content_scripts": [{
	"js":  ["addJXLsupport.js","b64wasm.js"],
	"matches": ["<all_urls>"],
	"run_at": "document_start",
	"all_frames": true
    }]
}